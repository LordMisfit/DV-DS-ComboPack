		if (type == 0) range = 64; // Punch [normal]
		if (type == 1) range = 58; // Punch [uppercut]
		if (type == 2) range = 72; // Gauntlet Punch [normal]
		if (type == 3) range = 80; // Gauntlet Punch [alt-fire]
		if (type == 4) range = 104; // Kick
		if (type == 5) range = 73; // Chainsaw
