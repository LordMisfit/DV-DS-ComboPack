{
	default
	{
		Tag "Sigil Spread Shot";
		+SPECTRAL;
		DamageType "Sigil";
		DamageFunction (finaldamagedealt); // 70
		Speed 30;
		Radius 8;
		Height 16;
		Projectile;
	}
	States
	{
		Spawn:
			ZOT3 A 0 Bright NoDelay { A_GetSigilDamage(1,0); }
			ZOT3 ABCDE 4 Bright
			{
				A_GetSigilDamage(1,0);
			}
			Loop;
	}
}
