	action void A_SceptreSpinningShield(double angoffset = 0.0, double ptcoffset = 0.0)
	{
		bool spawned;
		actor mo;
		double xpos, ypos, zpos, xvel, yvel, zvel, ang, ptc;
		
		let baseRange = A_GetSceptreRange(2);
		let sceptrelevel = CallACS("OtherVarCheckDECORATE",11);
		string reflpiece; string blckpiece;
		if (sceptrelevel >= 0 && sceptrelevel <= 8) { reflpiece = "ShieldPartSpinAttack1D"; blckpiece = "ShieldPartSpinAttack1"; }
		if (sceptrelevel >= 9 && sceptrelevel <= 16) { reflpiece = "ShieldPartSpinAttack2D"; blckpiece = "ShieldPartSpinAttack2"; }
		let maxpieces = baseRange / 32;
		for (let piece = 0; piece < maxpieces; piece++)
		{
			xpos = 0.0 + (cos(-pitch) * A_SetShieldPieceDist(baseRange - (piece * 32)));
			ypos = 0.0;
			zpos = 20.0 + (sin(-pitch) * A_SetShieldPieceDist(baseRange - (piece * 32)));
			xvel = 0.0;
			yvel = 0.0;
			zvel = 0.0;
			ang = 0.0;
			[spawned, mo] = A_SpawnItemEx(
					piece == 0 ? reflpiece : blckpiece,
					xpos, // xpos
					ypos, // ypos
					zpos, // zpos
					xvel, // xvel
					yvel, // yvel
					zvel, // zvel
					ang, // angle
					SXF_NOCHECKPOSITION, // Flags
					0, // FailChance
					0 // TID
			);
			if (mo) A_SceptreShieldPieceSet(mo, xpos, ypos, zpos, angoffset, ptcoffset);
			
			xpos = 0.0 + (cos(-pitch) * A_SetShieldPieceDist(baseRange - (piece * 32)));
			ypos = 0.0;
			zpos = 44.0 + (sin(-pitch) * A_SetShieldPieceDist(baseRange - (piece * 32)));
			xvel = 0.0;
			yvel = 0.0;
			zvel = 0.0;
			ang = 0.0;
			[spawned, mo] = A_SpawnItemEx(
					piece == 0 ? reflpiece : blckpiece,
					xpos, // xpos
					ypos, // ypos
					zpos, // zpos
					xvel, // xvel
					yvel, // yvel
					zvel, // zvel
					ang, // angle
					SXF_NOCHECKPOSITION, // Flags
					0, // FailChance
					0 // TID
			);
			if (mo) A_SceptreShieldPieceSet(mo, xpos, ypos, zpos, angoffset, ptcoffset);
		}
	}

