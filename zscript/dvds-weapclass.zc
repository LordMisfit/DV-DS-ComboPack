			//Console.Printf("heatdivisior: %d [COOLING]", heatdivisior);
		}
		CallACS("MiscVarSetDECORATE",191,heatdivisior);
	}
	
	
	action void A_SceptreShieldPieceSet(actor mo, double xpos = 0.0, double ypos = 0.0, double zpos = 0.0, double ang = 0.0, double ptc = 0.0)
	{
		if (mo)
		{
			if (dvdsdebug_showvisibleshieldparts) Console.Printf("[mo]: %s", mo.GetClassName());
			ShieldPartBase SHmo;
			SHmo = ShieldPartBase(mo);
			if (SHmo)
			{
				actor playera = players[0].mo;
				if (dvdsdebug_showvisibleshieldparts) Console.Printf("[SHmo]: %s", SHmo.GetClassName());
				SHmo.attachx = xpos;
				SHmo.attachy = ypos;
				SHmo.attachz = zpos;
				SHmo.attachang = ang;
				SHmo.attachptc = ptc;
				SHmo.shieldee = playera;
				SHmo.target = playera;
				if (dvdsdebug_showvisibleshieldparts) Console.Printf("attachx: %.2f, attachy: %.2f, attachz: %.2f, attachang: %.2f, attachptc: %.2f, shieldee: %s", SHmo.attachx, SHmo.attachy, SHmo.attachz, SHmo.attachang, SHmo.attachptc, SHmo.shieldee.GetClassName());
				if (dvdsdebug_showvisibleshieldparts) Console.Printf("posx: %.2f, posy: %.2f, posz: %.2f, angle: %.2f, pitch: %.2f", (SHmo.pos.x - playera.pos.x), (SHmo.pos.y - playera.pos.y), (SHmo.pos.z - playera.pos.z), (SHmo.angle - playera.angle), (SHmo.pitch - playera.pitch));
			}
		}
	}
	
