		let baseRange = A_GetSceptreRange(0);
		let sceptrelevel = CallACS("OtherVarCheckDECORATE",11);
		string reflpiece; string blckpiece;
		if (sceptrelevel >= 0 && sceptrelevel <= 4) { reflpiece = "ShieldPartSpinAttack1D"; blckpiece = "ShieldPartSpinAttack1"; }
		if (sceptrelevel >= 5 && sceptrelevel <= 9) { reflpiece = "ShieldPartSpinAttack2D"; blckpiece = "ShieldPartSpinAttack2"; }
		if (sceptrelevel >= 10 && sceptrelevel <= 13) { reflpiece = "ShieldPartSpinAttack3D"; blckpiece = "ShieldPartSpinAttack3"; }
		if (sceptrelevel >= 14 && sceptrelevel <= 16) { reflpiece = "ShieldPartSpinAttack4D"; blckpiece = "ShieldPartSpinAttack4"; }
		let maxpieces = baseRange / 32;
		for (let piece = 0; piece < maxpieces; piece++)
