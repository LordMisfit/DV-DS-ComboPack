				return resolvestate(null);
			}
			goto Spawn;
		Piece3:
			SGLP DDDD 1 bright
			{
				A_SetTics(random(2,3));
				if (CallACS("MiscVarCheckDECORATE",25038) != 2) { return resolvestate("Spawn"); }
				return resolvestate(null);
			}
			SGPG DDDD 1 bright
			{
				A_SetTics(random(2,3));
				if (CallACS("MiscVarCheckDECORATE",25038) != 2) { return resolvestate("Spawn"); }
				return resolvestate(null);
			}
			goto Spawn;
		Piece4:
			SGLP CCCC 1 bright
			{
				A_SetTics(random(2,3));
				if (CallACS("MiscVarCheckDECORATE",25038) != 3) { return resolvestate("Spawn"); }
				return resolvestate(null);
			}
			SGPG CCCC 1 bright
			{
				A_SetTics(random(2,3));
				if (CallACS("MiscVarCheckDECORATE",25038) != 3) { return resolvestate("Spawn"); }
				return resolvestate(null);
			}
			goto Spawn;
		Piece5:
			SGLP EEEE 1 bright
			{
				A_SetTics(random(2,3));
				if (CallACS("MiscVarCheckDECORATE",25038) != 4) { return resolvestate("Spawn"); }
				return resolvestate(null);
			}
			SGPG EEEE 1 bright
			{
				A_SetTics(random(2,3));
				if (CallACS("MiscVarCheckDECORATE",25038) != 4) { return resolvestate("Spawn"); }
				return resolvestate(null);
			}
			goto Spawn;
		Pickup:
			TNT1 A 0
			{
				//A_LogInt(CallACS("MiscVarCheckDECORATE",25038));
				if (CallACS("MiscVarCheckDECORATE",25038) == 0)
