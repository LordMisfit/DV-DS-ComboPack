						}
					}
				}
				
				tempaeonpts = 0;
				comboamount = 0;
				CallACS("MiscVarSetDECORATE",158,0); // Set kill counter back to 0
				CallACS("MiscVarSetDECORATE",160,0); // Set EXP Combo Amount back to 0
			}
