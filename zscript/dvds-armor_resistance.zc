				if (debugcrithits > 0) Console.Printf("    \cwCrit Rate % [STEP 4a - Acceleratle Check]: %d,\c- Added: %d", CriticalHitRate, SkillFactor);
			}
			SkillFactor = 0;
			// Deceleratle (-10% of remaining amount uncovered)
