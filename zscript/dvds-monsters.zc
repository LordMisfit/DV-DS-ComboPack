			CritResistChance *= CritResistChanceReduce;

			// Type Detection
			if (mod == 'Eldritch') CritResistChance /= 4;
			// Ripper Detection
			if (infl == "RagingSpirit") 
			{
				if (random(1,24) != 1) CritResistChance = 200.0;
