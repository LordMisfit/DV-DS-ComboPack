	Actor A_DVDSSpawnProjectile(class<Actor> missiletype, double spawnheight = 32, double spawnofs_xy = 0, double angle = 0, int flags = 0, double pitch = 0, int ptr = AAPTR_TARGET)
	{
		/*
		PARAM_SELF_PROLOGUE(AActor);
