							if (ActorClass == "EntityAscended" && random(1,8) <= basedroprate) { A_SpawnItemEx(MiscCheck,frandom(-DropX,DropX),frandom(-DropY,DropY),frandom(-DropZ,DropZ),frandom(-DropVels1,DropVels1),frandom(-DropVels1,DropVels1),frandom(-DropVels2,DropVels2),0,SXF_NOCHECKPOSITION,0); A_RareDropSnd(nm, GetClassName()); }
						}
					}
