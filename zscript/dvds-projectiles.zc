				user_radiusdmg *= 0.5;
				user_radiussize *= 0.5;
				A_Explode(user_radiusdmg,user_radiussize,XF_EXPLICITDAMAGETYPE,false,(user_radiussize*0.5),0,0,"BulletPuff","BFG");
			}
