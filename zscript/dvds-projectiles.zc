				A_ScaleVelocity(0);
				A_StartSound("weapons/bfgx",CHAN_7,CHANF_DEFAULT,0.5);
				A_Quake(1, 12, 0, 384, "");
				A_Quake(5, 12, 0, 256, "");
			}
			BFE1 AAAABBBBCCCCDDDDEEEEFFFF 1 bright
			{
				user_projtimer++;
				bFORCERADIUSDMG = false;
				if ((user_projtimer % 8) == 0) { bFORCERADIUSDMG = true; }

				bFORCERADIUSDMG = true;
