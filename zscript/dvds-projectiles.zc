		Spawn:
			APLS AAAAABBBBB 1 Bright;
			Loop;
		Death:
			APBX AAAAABBBBBCCCCCDDDDDEEEEE 1 Bright
			{
				user_projtimer++;
				if (user_projtimer > 1)
				{
					bNODAMAGETHRUST = false;
					bPAINLESS = true;
				}
				if ((user_projtimer % 4) == 0) 
