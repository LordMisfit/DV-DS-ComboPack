		//**************************************************************************************************************
		//*Flicker the Player when they survive a mortal hit (mercy invincibility) and perform actions related to perks*
		//**************************************************************************************************************
		// [194] Mercy Invincibility "Flag"
		// [195] Mercy Invincibility Timer
		// [196] Mercy Invincibility 'Modulus/Remainder'
		// Death Rage Countdown Checks
		double DeathRageMaxTimer = 1;
		double DeathRageTimer = 1;
		double DRTimerRatio = 1.0;
		if (CallACS("MiscVarCheckDECORATE",9043) > 0)
		{
			DeathRageTimer = CallACS("MiscVarCheckDECORATE",9043);
			DeathRageMaxTimer = CallACS("MiscVarCheckDECORATE",9042);
			DRTimerRatio = (DeathRageTimer / (DeathRageMaxTimer * 0.2));
			if (DRTimerRatio > 1.0) DRTimerRatio = 1.0;
			//Console.Printf("DRTimerRatio: %.8f (%d / %d)", DRTimerRatio, DeathRageTimer, DeathRageMaxTimer);
			A_SetBlend("d0 00 20", (0.525*DRTimerRatio), random(17,18));
			CallACS("MiscVarSetDECORATE",9043,-1,1); // Death Rage Timer
		}
		if (CallACS("MiscVarCheckDECORATE",9044) > 0) CallACS("MiscVarSetDECORATE",9044,-1,1); // Death Rage "Cooldown" Timer

		if (dvdsdebug_showplayerhealthsync) Console.Printf("\cxplayer.mo.health\c-: %d -- (\cyplayer.health\c-: %d) -- (\cwplayer.mo.Stamina\c-: %d)", player.mo.health, player.health, player.mo.stamina);
		int TempStartValue, TempEndValue, PerkFactor;
		int MaxHealthTrue = CallACS("StatCheck",1);
		int CurrentMana = CallACS("StatCheck",14);
		int MaxMana = CallACS("StatCheck",2);
		if (self.health == 1 && player.cheats & CF_BUDDHA && CallACS("MiscVarCheckDECORATE",241) > 1)
		{
			CallACS("MiscVarSetDECORATE",194,1); // Turns on Mercy Invincibility Flag
			CallACS("MiscVarSetDECORATE",195,1210); // 1.5 seconds [51.5 tics * 20]
			player.health = player.mo.health = 1;

			//*************************************************
			//*Check "Mortal Hit Survivability Psyche Up" Perk*
			//*************************************************
			if (CallACS("MiscVarCheckDECORATE",10037) > 0)
			{
				if (CharacterClass == 2) PerkFactor = 7500; else PerkFactor = 4000;
				if (random(1,10000) <= PerkFactor)
				{
					if (CallACS("MiscVarCheckDECORATE",10037) == 1 && CallACS("MiscVarCheckDECORATE",181) <= 1) ACS_NamedExecuteAlways("SetTensionLevel",0,1,0);
					if (CallACS("MiscVarCheckDECORATE",10037) == 2 && CallACS("MiscVarCheckDECORATE",181) <= 2) ACS_NamedExecuteAlways("SetTensionLevel",0,2,0);
					if (CallACS("MiscVarCheckDECORATE",10037) == 3 && CallACS("MiscVarCheckDECORATE",181) <= 3) ACS_NamedExecuteAlways("SetTensionLevel",0,3,0);
					if (CallACS("MiscVarCheckDECORATE",10037) == 4 && CallACS("MiscVarCheckDECORATE",181) <= 4) ACS_NamedExecuteAlways("SetTensionLevel",0,4,0);
					if (CallACS("MiscVarCheckDECORATE",10037) == 5 && CallACS("MiscVarCheckDECORATE",181) <= 5) ACS_NamedExecuteAlways("SetTensionLevel",0,5,0);
					if (CallACS("MiscVarCheckDECORATE",10037) == 6 && CallACS("MiscVarCheckDECORATE",181) <= 6) ACS_NamedExecuteAlways("SetTensionLevel",0,6,0);
					if (CallACS("MiscVarCheckDECORATE",10037) == 7 && CallACS("MiscVarCheckDECORATE",181) <= 7) ACS_NamedExecuteAlways("SetTensionLevel",0,7,0);
				}
			}
			
			//**********************************************************
			//*Check "Mortal Hit Survivability Mana Recovery [EX]" Perk*
			//**********************************************************
			if (CallACS("MiscVarCheckDECORATE",10039) > 0)
			{
				PerkFactor = 0;
				if (CallACS("MiscVarCheckDECORATE",10039) > 0) PerkFactor += 6666;
				if (CallACS("MiscVarCheckDECORATE",10040) > 0) PerkFactor += 3334;
				if (random(1,10000) <= PerkFactor)
				{
					PerkFactor = 0;
					if (CallACS("MiscVarCheckDECORATE",10039) > 0) PerkFactor += 25;
					if (CallACS("MiscVarCheckDECORATE",10040) > 0) PerkFactor += 50;
					TempStartValue = MaxMana * PerkFactor / 100;
					TempEndValue = MaxMana * (100 + PerkFactor) / 100;
					if (CurrentMana < TempEndValue) 
					{
						CurrentMana += TempStartValue;
						if (CurrentMana > TempEndValue) CurrentMana = TempEndValue;
						A_SetBlend("c0 00 c0", 0.75, random(17,18));
						CallACS("OtherVarSetDECORATE",2,CurrentMana);
					}
				}
			}
			
			//*******************************************************
			//*Check "Mortal Hit Survivability Blast Wave [EX]" Perk*
			//*******************************************************
			if (CallACS("MiscVarCheckDECORATE",10041) > 0)
			{
				PerkFactor = 0;
				if (CallACS("MiscVarCheckDECORATE",10041) > 0) PerkFactor += 5000;
				if (CallACS("MiscVarCheckDECORATE",10042) > 0) PerkFactor += 2500;
				if (random(1,10000) <= PerkFactor)
				{
					A_StartSound("MortalHit/BlastWave");
					A_SetBlend("ff c0 01", 0.75, random(17,18));
					int project_RealZ = (pos.z + 8) + (viewheight * 0.78);
					A_SpawnItemEx("PlayerMortalBlastRadius",0,0,project_RealZ,0.0,0.0,0.0,0,SXF_NOCHECKPOSITION|SXF_SETTRACER);
				}
			}
			
			//****************************************************
			//*Check "Mortal Hit Survivability 'Death Rage'" Perk*
			//****************************************************
			if (CallACS("MiscVarCheckDECORATE",10043) > 0 && CallACS("MiscVarCheckDECORATE",9044) <= 0)
			{
				if (CallACS("MiscVarCheckDECORATE",10043) == 1) PerkFactor = 1250;
				if (CallACS("MiscVarCheckDECORATE",10043) == 2) PerkFactor = 2000;
				if (CallACS("MiscVarCheckDECORATE",10043) == 3) PerkFactor = 2500;
				if (random(1,10000) <= PerkFactor)
				{
					A_StartSound("MortalHit/DeathRage");
					PerkFactor = random(17,18);
					if (CallACS("MiscVarCheckDECORATE",10043) == 1)
					{
						CallACS("MiscVarSetDECORATE",9043,(15 * 35),0); // Death Rage Timer
						CallACS("MiscVarSetDECORATE",9042,(15 * 35),0); // Death Rage Max Timer
						CallACS("MiscVarSetDECORATE",9044,(195 * 35),0); // Death Rage Cooldown Timer
					}
					if (CallACS("MiscVarCheckDECORATE",10043) == 2)
					{
						CallACS("MiscVarSetDECORATE",9043,((22 * 35) + PerkFactor),0); // Death Rage Timer
						CallACS("MiscVarSetDECORATE",9042,((22 * 35) + PerkFactor),0); // Death Rage Max Timer
						CallACS("MiscVarSetDECORATE",9044,((172 * 35) + PerkFactor),0); // Death Rage Cooldown Timer
					}
					if (CallACS("MiscVarCheckDECORATE",10043) == 3)
					{
						CallACS("MiscVarSetDECORATE",9043,(30 * 35),0); // Death Rage Timer
						CallACS("MiscVarSetDECORATE",9042,(30 * 35),0); // Death Rage Max Timer
						CallACS("MiscVarSetDECORATE",9044,(150 * 35),0); // Death Rage Cooldown Timer
					}
				}
			}
			
		}
		
		//******************
		//*Check Dash Timer*
		//******************
		int dashtrailinterval = level.time % random(1,4);
		int dashveltimer;
		int dashveltimer2;
		double dashtimermulti = 1.0;
		if (CountInv("SafeTeaShoes") > 0) dashtimermulti += 0.125;
		if (CountInv("CloakofEvasion") > 0) dashtimermulti += 0.0625;
		if (CountInv("DoveRibbon") > 0) dashtimermulti += 0.0625;

		int MaxDashTimer = 18;
		if (dvds_allowdashing > 0)
		{
			MaxDashTimer = 18 + (CallACS("MiscVarCheckDECORATE",31) / 32);
			if (dvds_allowdashing > 2) MaxDashTimer *= 0.5;
		}
		MaxDashTimer *= dashtimermulti;
		CallACS("MiscVarSetDECORATE",91,MaxDashTimer);
		dashveltimer = CallACS("MiscVarCheckDECORATE",166) % 2;
		dashveltimer2 = CallACS("MiscVarCheckDECORATE",166) % 1;

		if (CallACS("MiscVarCheckDECORATE",166) > 0) 
		{
			if (CallACS("MiscVarCheckDECORATE",166) == CallACS("MiscVarCheckDECORATE",91))
			{
				if (pos.z - GetZAt() > 0) firsttic = 1; else firsttic = 0;
			}
			if (dashtrailinterval == 0) A_SetInventory("PlayerDashing",1,AAPTR_PLAYER1); else A_SetInventory("PlayerDashing",0,AAPTR_PLAYER1); 
			bSHOOTABLE = 0;
			bVISIBILITYPULSE = 1;
			bTHRUACTORS = 1;
			if (CallACS("MenuVarCheckDECORATE",0) == 0)
			{
				CallACS("MiscVarSetDECORATE",166,-1,1);
				if (!(pos.z - GetZAt() > 0)) 
				{
					A_SpawnItemEx("DashPuff",0,0,8,0.0,0.0,0.0,0,SXF_NOCHECKPOSITION);
				}

				if (firsttic == 0) // On the Ground when Dashing began
				{
					if (dashveltimer == 0) { vel.x *= 1.0125; vel.y *= 1.0125; }
				}
				else // In the Air when Dashing began
				{
					if ((dvds_allowdashing == 2 || dvds_allowdashing == 4) && dashveltimer2 == 0) { vel.z = 0.0; }
				}
			}
		}
		else
		{
			firsttic = -1;
			A_SetInventory("PlayerDashing",0,AAPTR_PLAYER1); 
			bSHOOTABLE = 1;
			bVISIBILITYPULSE = 0;
			bTHRUACTORS = 0;
		}
