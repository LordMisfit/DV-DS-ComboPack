			{
				CallACS("StartTensionCountDown");
				A_GetPunchSwingRecoil(2);
				A_SpreadAuraCustomPunch(A_GetPunchDamage(0,0,false),1,CPF_NOTURN,"GauntletPuff",A_GetMeleeRange(2),0,0,"","","");
	
