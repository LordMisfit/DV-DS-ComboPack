	{
		Spawn:
			TPSN A 0 NoDelay
			{
				if (CountInv("AetheriusPowerStrength", AAPTR_PLAYER1) > 0) { A_ScaleVelocity(5); }
				A_GetPlayerAttackDamage(59,0);
				user_randompain = 44;
				user_randompain += (CallACS("MiscVarCheckDECORATE",10035) * 4);
				user_randompain += (CallACS("MiscVarCheckDECORATE",10036) * (4 * 5 / 2));
				user_randompain += (CallACS("MiscVarCheckDECORATE",10051) * random(3,5));
				user_randompain += (CallACS("MiscVarCheckDECORATE",10052) * (random(3,5) * 5 / 2));
				user_randompain *= 2;
				if (random(1,100) <= user_randompain) { A_ChangeFlag("FORCEPAIN", TRUE); }
			}
		Idle:
			TPSN AAAAAAAAA 2 { A_GetPlayerAttackDamage(59,0); }
			TPSN A 0 A_ChangeFlag ("NOGRAVITY",0)
			goto Falling
		Falling:
			TPSN A 2 { A_GetPlayerAttackDamage(59,0); }
			loop
		Death:
			TNT1 A 0
			TNT1 A 0 A_ChangeFlag ("NOGRAVITY",1)
			TNT1 A 0 A_PlaySound ("subweapons/miss")
			//SWMS ABC 3 bright
			TNT1 A 0 A_SpawnItemEx("PoisonNeedleAmmo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0,tid)
			stop
		Crash:
			TNT1 A 0
			TNT1 A 0 A_ChangeFlag ("NOGRAVITY",1)
			TNT1 A 0 A_PlaySound ("subweapons/miss")
			// SWMS ABC 3 bright
			TNT1 A 0 A_SpawnItemEx("PoisonNeedleAmmo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0,tid)
			stop
		XDeath:
			TNT1 A 0
			TNT1 A 0 A_ChangeFlag ("NOGRAVITY",1)
			TNT1 A 0 A_PlaySound ("subweapons/hit")
			//SWHT ABC 3 bright
			TNT1 A 0 A_SpawnItemEx("PoisonNeedleAmmo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0,tid)
			stop
	}
}

