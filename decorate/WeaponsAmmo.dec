			}
			LGUN A 1 
			loop
		Ready:
			UNMA A 1 
			{
				A_SetInventory("WeapCrisis",0);
				A_WeaponReady(WRF_ALLOWUSER4);
			}
			Loop
		Deselect:
			UNMA A 1 
			{
				A_DVDSLower(0,10035,2);
