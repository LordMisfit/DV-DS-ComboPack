				if (CallACS("MiscVarCheckDECORATE",10023) >= 1) { A_SetTics(random(11,12)); }
			}
			goto Ready
		Flash:
			TNT1 A 5 bright A_Light1
			TNT1 A 5 bright A_Light2
			TNT1 A 0 bright A_Light0
			Goto LightDone
		
