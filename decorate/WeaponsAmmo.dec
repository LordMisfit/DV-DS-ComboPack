Actor Chaingun2 : MarineWeapon
{
	Weapon.Kickback 100
	Weapon.SelectionOrder 700
	Weapon.AmmoUse 1
	Weapon.AmmoGive 20
	Weapon.AmmoType "Clip"
	Obituary "$OB_MPCHAINGUN" // "%o was mowed down by %k's chaingun."
	Tag "$TAG_CHAINGUN"
	Inventory.PickupMessage "Picked up a \cuchaingun\c-! \cx[Slot 4]\c-"
	States
	{
		Spawn:
			MGUN A -1
			Stop
		Ready:
			CHGG A 1 A_WeaponReady(WRF_ALLOWUSER1)
			Loop
		Deselect:
			CHGG A 1
			{
				A_DVDSLower(CallACS("MiscVarCheckDECORATE",10019)*3);
				A_DVDSLower(CallACS("MiscVarCheckDECORATE",10020)*7.5);
				A_DVDSLower(CallACS("MiscVarCheckDECORATE",10051)*2);
				A_DVDSLower(CallACS("MiscVarCheckDECORATE",10052)*5);
				A_DVDSLower(18);
			}
			Loop
		Select:
			CHGG A 1 
			{
				A_DVDSRaise(CallACS("MiscVarCheckDECORATE",10019)*3);
				A_DVDSRaise(CallACS("MiscVarCheckDECORATE",10020)*7.5);
				A_DVDSRaise(CallACS("MiscVarCheckDECORATE",10051)*2);
				A_DVDSRaise(CallACS("MiscVarCheckDECORATE",10052)*5);
				A_DVDSRaise(18);
			}
			Loop

		Fire:
			CHGG A 4 
			{
				if (CallACS("MiscVarCheckDECORATE",10019) >= 2) { A_SetTics(3); }
				if (CallACS("MiscVarCheckDECORATE",10019) >= 4) { A_SetTics(2); }
				A_GunFlash("Flash");
				A_PlaySound("weapons/chngun", CHAN_WEAPON);
				CallACS("StartTensionCountDown");
				A_SpreadAuraFireBullets(A_GetHitScanXSpread(11), A_GetHitScanYSpread(11), 1, A_GetPlayerAttackDamage(5,1), "BulletPuff",FBF_NORANDOM|FBF_NORANDOMPUFFZ,FBF_USEAMMO|FBF_NORANDOM|FBF_NORANDOMPUFFZ,0,"",0,0,1,"Clip");
			}
			CHGG B 4
			{
				if (CallACS("MiscVarCheckDECORATE",10019) >= 2) { A_SetTics(3); }
				if (CallACS("MiscVarCheckDECORATE",10019) >= 4) { A_SetTics(2); }
				A_GunFlash("Flash2");
				A_PlaySound("weapons/chngun", CHAN_WEAPON);
				CallACS("StartTensionCountDown");
				A_SpreadAuraFireBullets(A_GetHitScanXSpread(11), A_GetHitScanYSpread(11), 1, A_GetPlayerAttackDamage(5,1), "BulletPuff",FBF_NORANDOM|FBF_NORANDOMPUFFZ,FBF_USEAMMO|FBF_NORANDOM|FBF_NORANDOMPUFFZ,0,"",0,0,1,"Clip");
			}
			CHGG B 0 A_ReFire
			Goto Ready
		Flash:
			CHGF A 4 Bright 
			{
				if (CallACS("MiscVarCheckDECORATE",10019) >= 2) { A_SetTics(3); }
				if (CallACS("MiscVarCheckDECORATE",10019) >= 4) { A_SetTics(2); }
				A_Light1;
			}
			Goto LightDone
		Flash2:
			CHGF B 4 Bright
			{
				if (CallACS("MiscVarCheckDECORATE",10019) >= 2) { A_SetTics(3); }
				if (CallACS("MiscVarCheckDECORATE",10019) >= 4) { A_SetTics(2); }
				A_Light1;
			}
			Goto LightDone
	}
}
