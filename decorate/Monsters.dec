				A_CrisisMeleeAttack((random(1,10)*randompick(4,4,4,4,6,6,8)),"none","none","Melee",true); // A_SargAttack
			}
			Goto See

		Pain:
			DOGS H 2
			DOGS H 2 A_DVDSPain
			Goto See
		Pain.Fist: 
			DOGS H 2
			DOGS H 2 A_DVDSPain
			DOGS HH 2 
			Goto See 
		Pain.Kick: 
			DOGS H 2
			DOGS H 2 A_DVDSPain
			DOGS HHHH 2 
			Goto See 

		Death:
