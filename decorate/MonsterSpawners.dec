// Baron Of Hell
actor BaronofHellSpawner : BaseMonsterSpawner replaces BaronofHell
{
 //$Sprite "RNMSN0"
 Radius 24
 Height 64
 +ISMONSTER
 +RANDOMIZE
 states
 {
    UpgradeClass4:
     TNT1 A 0 A_SpawnItemEx("Hierophant",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
     stop

    UpgradeClass3:
     TNT1 A 0 A_JumpIf( random(1,1024) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeClass4")
     TNT1 A 0 A_SpawnItemEx("PyroDemon",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
     stop

    UpgradeClass2:
     TNT1 A 0 A_JumpIf( random(1,384) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeClass3")
     TNT1 A 0 A_Jump(256, "Bruiser","Archon","CyBruiser")

     Bruiser:
      TNT1 A 0 A_SpawnItemEx("BruiserDemon",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
      stop
     Archon:
      TNT1 A 0 A_SpawnItemEx("ArchonofHell",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
      stop
     CyBruiser:
      TNT1 A 0 A_SpawnItemEx("CyBruiser",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
      stop

    UpgradeClass:
     TNT1 A 0 A_JumpIf( random(1,320) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeClass2")
     TNT1 A 0 A_SpawnItemEx("Belphegor",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
     stop

    UpgradeToFlyingBalrog:
     TNT1 A 0 A_SpawnItemEx("FlyingBalrog",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
     TNT1 A 0
     stop

    UpgradeToHellWarrior:
     TNT1 A 0 A_SpawnItemEx("HellWarrior",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
     TNT1 A 0
     stop

  Spawn:
   TNT1 A 0 
   TNT1 A 0 A_JumpIf( random(1,256) <= 8 && ACS_NamedExecuteWithResult("CheckModType") == 0 && ACS_NamedExecuteWithResult("CheckDVIIMonstersOption") == 1, "UpgradeToFlyingBalrog")
   TNT1 A 0 A_JumpIf( random(1,256) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeClass")
   TNT1 A 0 A_JumpIf( random(1,4096) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeToHellWarrior")
   TNT1 A 0 A_SpawnItemEx("DSBaronofHell",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
   stop
 }
}
actor StealthBaronSpawner : BaronofHellSpawner replaces StealthBaron { }

