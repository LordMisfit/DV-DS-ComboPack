// Demon
actor DemonSpawner : BaseMonsterSpawner replaces Demon
{
  //$Sprite "RNMSE0"
  Radius 30
  Height 56
  +ISMONSTER
  +RANDOMIZE
  states
  {
      UpgradeSpectre:
       TNT1 A 0 A_JumpIf( random(1,512) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeChaingunSpectre")
       TNT1 A 0 A_Jump(16,"SpawnChaingunSpectre")
       goto SpawnBloodSpectre

       SpawnBloodSpectre:
        TNT1 A 0 A_SpawnItemEx("BloodSpectre",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
        stop
       SpawnChaingunSpectre:
        TNT1 A 0 A_SpawnItemEx("ChaingunSpectre",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
        stop

       UpgradeChaingunSpectre:
        TNT1 A 0 A_SpawnItemEx("ChaingunSpectre",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
        stop
	   
	   
    MakeSpectre:
     TNT1 A 0 A_JumpIf( random(1,256) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeSpectre")
     TNT1 A 0 A_SpawnItemEx("DSSpectre",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
     stop

    UpgradeClass:
     TNT1 A 0 A_JumpIf( random(1,512) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeClassChaingun")
     TNT1 A 0 A_Jump(16,"SpawnChaingunDemon")
     goto SpawnBloodDemon

     SpawnBloodDemon:
      TNT1 A 0 A_SpawnItemEx("BloodDemon",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
      stop
     SpawnChaingunDemon:
      TNT1 A 0 A_SpawnItemEx("ChaingunDemon",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
      stop

     UpgradeClassChaingun:
      TNT1 A 0 A_SpawnItemEx("ChaingunDemon",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
      stop

    UpgradeToHellCleric:
     TNT1 A 0 A_SpawnItemEx("HellCleric",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
     TNT1 A 0
     stop

  Spawn:
   TNT1 A 0 
   TNT1 A 0 A_JumpIf( random(1,256) <= 16 && ACS_NamedExecuteWithResult("CheckModType") == 0 && ACS_NamedExecuteWithResult("CheckDVIIMonstersOption") == 1, "UpgradeToHellCleric")
   TNT1 A 0 A_JumpIf( random(1,256) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "MakeSpectre")
   TNT1 A 0 A_JumpIf( random(1,256) <= ACS_NamedExecuteWithResult("GetEvolveChance") , "UpgradeClass")
   TNT1 A 0 A_SpawnItemEx("DSDemon",0,0,0,0,0,0,0,SpawnerFlags,0,tid)
   stop
  }
}

