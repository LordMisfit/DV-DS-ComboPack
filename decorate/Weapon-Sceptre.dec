// SceptrePartVisual[x][y] represents the visual sprites of the sceptre mid-twirl
actor SceptrePartVisualBase
{
  Health 2147483646
  Painchance 255
  Radius 30 // 16
  Height 30 // 16
  Scale 0.5
  Alpha 0.75
  RenderStyle Translucent
  Species "Players"
  +NOINTERACTION
  +FLOORCLIP
  +NOTIMEFREEZE
  +REFLECTIVE +AIMREFLECT
  +THRUSPECIES
  +SOLID
  +NOBLOOD
  +NOBLOODDECALS
  +NOTELESTOMP
  +DONTBLAST
  +DONTRIP
  +NOTELEPORT
  +NODAMAGE
  +NOGRAVITY
  +BUDDHA

  States
  {
  Spawn:
    STR3 A 2 bright
    stop
  Pain.Magic:
    TNT1 A 0 
    stop
  Pain.Sceptre:
    TNT1 A 0 
    stop
  Pain:
    TNT1 A 0 bright A_PlaySound ("SceptreBlocked") // A_PlaySound ("ForceBarrier/Hit")
    stop
  Death:
    TNT1 A 0
    stop
  }
}

