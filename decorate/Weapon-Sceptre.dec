// SceptrePartVisual[x][y] represents the visual sprites of the sceptre mid-twirl
actor SceptrePartVisualBase
{
	Health 2147483646
	Painchance 255
	Radius 30 // 16
	Height 30 // 16
	Scale 0.5
	Alpha 0.75
	RenderStyle Add //Translucent
	Species "Players"
	+NOINTERACTION
	+FLOORCLIP
	+NOTIMEFREEZE
	+REFLECTIVE +AIMREFLECT
	+SOLID
	+NOBLOOD
	+NOBLOODDECALS
	+NOTELESTOMP
	+DONTBLAST
	+DONTRIP
	+NOTELEPORT
	+NODAMAGE
	+NOGRAVITY
	+BUDDHA
	+THRUSPECIES +MTHRUSPECIES

	States
	{
		Spawn:
			TNT1 A 0 NoDelay 
			{
				if (CountInv("InvisiblitySpell",AAPTR_PLAYER1) > 0) { A_SetTranslucent(0.075,0); } else { A_SetTranslucent(0.75,0); }
				if (CountInv("PlayingDoom64",AAPTR_PLAYER1) > 0) { A_SetScale(ScaleX*1.2,ScaleY*1.2); A_SetSize(36,36); }
			}
			STR3 A 2 bright
			stop
		Pain.Magic:
			TNT1 A 0 
			stop
		Pain.Sceptre:
			TNT1 A 0 
			stop
		Pain:
			TNT1 A 0 bright 
			{
				A_PlaySound ("SceptreBlocked");
			}
			stop
		Death:
			TNT1 A 0
			stop
	}
}

