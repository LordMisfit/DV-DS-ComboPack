// Sceptre Cruft and Stuff ;P
actor ShieldPart // The Main segment of the sceptre-shield which deflects/blocks attacks
{
	Species "Players"
	Health 2147483646
	Painchance 256
	Radius 32 // out of 32
	Height 32 // out of 32
	Scale 0.50
	RenderStyle Add // Add
	Alpha 0.0 // 0.003125
	+NOTARGET +NEVERTARGET
	+BUDDHA
	+DONTBLAST
	+SHOOTABLE
	+NOGRAVITY
	+NOTELEPORT
	+NODAMAGE
	+DONTRIP
	+NOBLOOD
	+NOBLOODDECALS
	+FLOORCLIP
	+REFLECTIVE +AIMREFLECT
	+NOTELESTOMP
	+NOTIMEFREEZE
	+THRUSPECIES +MTHRUSPECIES
	States
	{
		Spawn:
			HEXA C 2 bright NoDelay 
			{
				if (GetCVar("dvdsdebug_shieldactorghosts") > 0) { A_FadeTo(0.125); }
				if (CountInv("PlayingDoom64",AAPTR_PLAYER1) > 0) { A_SetSize(19,19); }
				//A_RadiusThrust(256,8,RTF_NOIMPACTDAMAGE,8)
			}
			stop
		Pain.Magic:
			TNT1 A 0 
			stop
		Pain.Sceptre:
			TNT1 A 0 
			stop
		Pain:
			TNT1 A 0 bright 
			{
				A_PlaySound("SceptreBlocked");
			}
			stop
		Death:
			TNT1 A 0
			stop
	}
}

