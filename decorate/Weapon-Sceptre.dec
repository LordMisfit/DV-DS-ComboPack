actor ShieldSpawner
{
  Radius 1
  Height 1
  +NOGRAVITY
  +NOINTERACTION
  +NOTIMEFREEZE
  States

  {
  Spawn:
    TNT1 A 0 NoDelay
	{
	 if (random(1,256) >= 256)
	 {
	  if (CountInv("PlayingStrife",AAPTR_PLAYER1) > 0) { } // Strife mode makes the punch work like the Punch Dagger where it NEVER makes sound
	  else
	  {
	   A_AlertMonsters; // Sometimes a swing will be enough to wake up all monsters in the area.
	  }
	 }
	}
  Finish:
    TNT1 A 0 A_Jump(1,25)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner6upNoReflect", 0, 0, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner5upNoReflect", 16, -8, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner5upNoReflect", -16, -8, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner4upNoReflect", 32, -21, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner4upNoReflect", -32, -21, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)

    TNT1 A 0 A_Jump(6,19)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner6upNoReflect", 0, 8, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner5upNoReflect", 16, 0, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner5upNoReflect", -16, 0, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner4upNoReflect", 32, -11, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner4upNoReflect", -32, -11, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)

    TNT1 A 0 A_Jump(21,13)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner6up", 0, 16, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner5up", 16, 8, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner5up", -16, 8, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner4up", 32, -3, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner4up", -32, -3, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)

    TNT1 A 0 A_Jump(60,7)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner3upNoReflect", 40, -30, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner3upNoReflect", -40, -30, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner3upNoReflect", 40, -25, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner3upNoReflect", -40, -25, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner3up", 40, -16, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)
    TNT1 A 0 A_SpawnItemEx ("ShieldSpawner3up", -40, -16, 0, 0, 0, 0, 0, SXF_TRANSFERPOINTERS)

    TNT1 A 0
    stop
  }
}

