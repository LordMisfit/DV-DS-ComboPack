actor GreenArmorDS : AetheriusCustomInventory
{
	Game Doom
	Radius 20
	Height 16
	Inventory.Pickupsound "GetArmorFull"
	Inventory.Pickupmessage "Picked up the \cdsecurity armor\c-."
	States
	{
		Spawn:
			TNT1 A 0 NoDelay
			{
				if (A_ClassicDoom2ArmorCheck == 2) 
				{ 
					A_SpawnItemEx("MaxHealthBonus",0,0,0,random(-2,2),random(-2,2),random(-2,2),0,0,tid);
					A_SpawnItemEx("MaxHealthBonus",0,0,0,random(-2,2),random(-2,2),random(-2,2),0,0,0);
					A_Destroy;
				} 
				else 
				if (A_ClassicDoom2ArmorCheck == 5) 
				{
					A_SpawnItemEx("MaxHealthBonus",0,0,0,random(-2,2),random(-2,2),random(-2,2),0,0,tid);
					A_SpawnItemEx("MaxHealthBonus",0,0,0,random(-2,2),random(-2,2),random(-2,2),0,0,0);
					A_SpawnItemEx("FloraRandomArmorDoom2",0,0,0,0,0,0,0,0,0);
					A_Destroy;
				} 
			}
		Idle:
			ARM1 AAAAAA 1
			{
				A_SetScale(1.0);
				if (A_CheckArmorLevelsOrb(1)) { } else { return state("SpiritIdle"); }
				return state("");
			}
			ARM1 BBBBBB 1 bright
			{
				A_SetScale(1.0);
				if (A_CheckArmorLevelsOrb(1)) { } else { return state("SpiritIdle"); }
				return state("");
			}
			loop
		SpiritIdle:
			SAS1 AAAABBBBCCCCDDDDEEEEFFFFGGGGHHHHIIIIHHHHGGGGFFFFEEEEDDDDCCCCBBBBAAAA 1 bright
			{
				A_SetScale(0.67);
				if (A_CheckArmorLevelsOrb(1)) { return state("Idle"); }
				return state("");
			}
			loop
		
		Pickup:
			TNT1 A 0 
			{ 
				if (A_CheckArmorLevels(1)) 
				{
					return state("Success"); 
				}
				else
				{
					if (A_PickupArmorMainOrb(2,1,1) > 0) { return state("OrbSuccess"); }
					else
					{
						if (A_PickupArmorMainOrb(3,1,1) > 0) { return state("OrbSuccess"); }
						else
						{
							if (A_PickupArmorMainOrb(4,1,1) > 0) { return state("OrbSuccess"); }
							else
							{
								if (A_PickupArmorMainOrb(5,1,1) > 0) { return state("OrbSuccess"); }
								else
								{
									if (A_PickupArmorMainOrb(6,1,1) > 0) { return state("OrbSuccess"); }
									return state(""); 
								}
								return state(""); 
							}
							return state(""); 
						}
						return state(""); 
					}
					return state(""); 
				}
				return state(""); 
			}
			Fail
		
		Success:
			TNT1 A 0 
			{
				A_StartSound("generalequip",5);
				A_PickupArmorMain(1);
			}
			Stop
		OrbSuccess:
			TNT1 A 0
			{
				if (A_CheckArmorLevels(2)) { A_PickupArmorMainOrb(2,1); }
				else
				{
					if (A_CheckArmorLevels(3)) { A_PickupArmorMainOrb(3,1); }
					else
					{
						if (A_CheckArmorLevels(4)) { A_PickupArmorMainOrb(4,1); }
						else
						{
							if (A_CheckArmorLevels(5)) { A_PickupArmorMainOrb(5,1); }
							else
							{
								if (A_CheckArmorLevels(6)) { A_PickupArmorMainOrb(6,1); }
							}
						}
					}
				}
			}
			stop
	}
}

