					{
						if (random(1,256) <= 1)
						{
							A_SpawnItemEx("DoomSpellMana3",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0,tid);
