// Bullet Tracers
ACTOR ZombieBulletTracer : AetheriusBulletTracerZSC
{
	Tag "shot"
	Damage ((Random(1,5) * 3))
	Damagetype "Bullet"
	Radius 1
	Height 1
	Mass 5
	Speed 160 // 80
	PROJECTILE
	+DONTREFLECT
	+BLOODSPLATTER
	+NODAMAGETHRUST
	Renderstyle Add
	Alpha 1.0
	Scale 0.5
	decal BulletChip
	States
	{
		Spawn:
			TNT1 A 0 NoDelay bright
		Idle:
			TNT1 A 1 bright A_SpawnTracerTrail("ZombieBulletTracerTrail",6,-2.5)
			loop
		XDeath:
			TNT1 A 1 //A_SpawnItem("BulletPuff")
			stop
		Death:
		Crash:
			TNT1 A 1 A_SpawnItem("BulletPuff")
			stop
	}
}

ACTOR ZombieBulletTracerCrisis : ZombieBulletTracer
{
	Damage ((Random(1,5) * 3) * CrisisMultiplier)
	States
	{
		Spawn:
