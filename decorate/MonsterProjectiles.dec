// Arachnotron Shots
ACTOR ArachnotronPlasmaNew : AetheriusProjectileZSC replaces ArachnotronPlasma
{
	Tag "Plasma Ball"
	Radius 13
	Height 8
	Speed 25
	Damage 5
	Projectile
	+RANDOMIZE
	RenderStyle Add 
	Alpha 0.75
	SeeSound "baby/attack"
	DeathSound "baby/shotx"
	States
	{
		Spawn:
			APLS AAAAABBBBB 1 Bright
			Loop
		Death:
			APBX AAAAABBBBBCCCCCDDDDDEEEEE 1 Bright
			{
				user_projtimer++;
				if (user_projtimer > 1)
				{
					A_ChangeFlag("NODAMAGETHRUST",0);
					A_ChangeFlag("PAINLESS",1);
				}
				if ((user_projtimer % 4) == 0) 
				{ 
					if (random(1,16) <= 2) { A_ChangeFlag("FORCERADIUSDMG",1); }
					A_Explode(5,16,0,0,8);
				}
			}
			Stop
	}
}

actor ArachnotronPlasmaCrisis : ArachnotronPlasmaNew
{
	Damage ((5 * CrisisMultiplier) * random(1,8))
	States
	{
		Spawn:
			APLS A 0
			APLS AAAAABBBBB 1 Bright
			{
				user_crisistimer += 1;
				if (user_crisistimer >= 2) 
				{ 
					A_GenerateCrisisParticle; 
					user_crisistimer = 0;
				}
			}
			Loop
		Death:
			APBX AAAAABBBBBCCCCCDDDDDEEEEE 1 Bright
			{
				user_projtimer++;
				if (user_projtimer > 1)
				{
					A_ChangeFlag("NODAMAGETHRUST",0);
					A_ChangeFlag("PAINLESS",1);
				}
				if ((user_projtimer % 4) == 0) 
				{ 
					if (random(1,16) <= 2) { A_ChangeFlag("FORCERADIUSDMG",1); }
					A_Explode((5*CrisisMultiplier),16,0,0,8);
				}
			}
			Stop
	}
}

