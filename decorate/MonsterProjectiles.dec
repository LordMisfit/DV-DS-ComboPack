Actor PlasmaBallMonster : PlasmaBallBase
{
	Tag "Plasma Ball"
	Damage (5 * random(1,8))
	DamageType "Plasma"
	States
	{
		Death:
			PLSE AAAABBBBCCCCDDDDEEEE 1 Bright
			{
				user_projtimer++;
				if (user_projtimer > 1)
				{
					A_ChangeFlag("NODAMAGETHRUST",0);
					A_ChangeFlag("PAINLESS",1);
				}

				if ((user_projtimer % 4) == 0) 
				{ 
					if (random(1,16) <= 2) { A_ChangeFlag("FORCERADIUSDMG",1); }
					A_Explode(5,16,0,0,8);
				}
			}
			PLSE BCDE 4 Bright
			Stop
	}
}

