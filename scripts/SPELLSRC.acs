			}
			MiscellaneousVars[196] = MiscellaneousVars[195] % 120;
			if (MiscellaneousVars[196] >= 0 && MiscellaneousVars[196] <= 14) flickeralpha = FixedMul(basealpha,0.25);
			if (MiscellaneousVars[196] >= 15 && MiscellaneousVars[196] <= 29) flickeralpha = FixedMul(basealpha,0.3125);
