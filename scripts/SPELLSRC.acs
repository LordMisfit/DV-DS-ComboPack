	int basealpha = 1.0;
	int tempalpha;
	int ghostdashvis = FixedMul(random(7,9),0.03125);
	//*******************************
	//**Mercy Invulnerablity Itself**
	//*******************************
	if (MiscellaneousVars[194] != 0) // Flickering
	{
		if (PowerUpTimer[1] > 0) basealpha = MiscFixedVars[101]; // Invisible
		
