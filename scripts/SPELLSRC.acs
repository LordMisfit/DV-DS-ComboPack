     DefenseFactor = FixedDiv(1.0, (AttackFactor+1.0)); //(1.0 / (AttackFactor + 1.0)) - 1.0;
     DefenseFactor -= 1.0;

     SpeedFactor = FixedDiv(AttackFactor,4.0);

