 if (index >= 0)
 {
  value *= 1.0;
  oldvalue = MiscFixedVars[index];
  if (type == 1) MiscFixedVars[index] += value; // adds to the value
  if (type == 2) MiscFixedVars[index] = value; // sets the value
  newvalue = MiscFixedVars[index];
  if (type == 0)
  {
