	if (ModType == 1) { forstart = 1100; forend = 1105; }
	if (ModType == 2) { forstart = 1200; forend = 1215; }
	SecretSplCostReduceBonus = 0;
	for (i = forstart; i < forend; i++) { SecretSplCostReduceBonus += SecretsFound[i]; }
	if (TrueGameSkill <= SKILL_VERY_EASY) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 7 / 60; // 11.67% of the total secrets found accumulate to the percentage
	if (TrueGameSkill == SKILL_EASY)  SecretSplCostReduceBonus = SecretSplCostReduceBonus * 3 / 30; // 10% of the total secrets found accumulate to the percentage
	if (TrueGameSkill == SKILL_NORMAL) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 1 / 12; // 8.33% of the total secrets found accumulate to the percentage
	if (TrueGameSkill == SKILL_HARD) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 2 / 30; // 6.67% of the total secrets found accumulate to the percentage
	if (TrueGameSkill == SKILL_VERY_HARD) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 3 / 60; // 5% of the total secrets found accumulate to the percentage
	if (TrueGameSkill == SKILL_EXTREME) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 2 / 50; // 4% of the total secrets found accumulate to the percentage
	if (TrueGameSkill == SKILL_HORRIBLE) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 3 / 100; // 3% of the total secrets found accumulate to the percentage
	if (TrueGameSkill == SKILL_IMPOSSIBLE) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 1 / 50; // 2% of the total secrets found accumulate to the percentage
	if (TrueGameSkill >= SKILL_OMEGA) SecretSplCostReduceBonus = SecretSplCostReduceBonus * 1 / 100; // 1% of the total secrets found accumulate to the percentage
	SecretSplCostReduceBonus *= -1; // Inverts the number to a "cost"
	SecretSplCostReduceBonus += 100; // Then adds 100 to it for the actual percentage factor

	if (SecretSplCostReduceBonus > 100) SecretSplCostReduceBonus = 100;
	if (CharacterClass == 2 || CharacterClass == 8 || CharacterClass == 9)
	{
		if (SecretSplCostReduceBonus < 50) SecretSplCostReduceBonus = 50; // Flora, Ailish or Petra
	}
	else
	{
		if (SecretSplCostReduceBonus < 63) SecretSplCostReduceBonus = 63; // Deggaris/Illucia
	}
	FinalSpellCost = FinalSpellCost * SecretSplCostReduceBonus / 100;
	if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [CostReduceBonus]");
 

	// Check for having The Ring in you inventory
	if (CheckActorInventory(Player1TID,"TheOneRing") > 0)
	{
		if (CharacterClass == 2) FinalSpellCost = FinalSpellCost * 13 / 16; // 81.25% MP Cost (Flora is the only one who doesn't have her MP Costs increased by the One Ring)
		else FinalSpellCost = FinalSpellCost * 5 / 4; // 125% MP Cost (Deggaris and Illucia)
		if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [Ring]");
	}
 
 int SpellCostMinimum;

	if (CharacterClass == 2)
	{
		Check1 = 0; // Check1 is an exact value
		Check2 = 100; // Check2 is a % value
		if (CheckActorInventory(Player1TID,"EvenstarNecklace") > 0) { Check1 += 10; Check2 -= 10; } // Evenstar Necklace [MP reduction of 10 + 10%]
		if (CheckActorInventory(Player1TID,"AnubisCufflinks") > 0) { Check1 += 2; Check2 -= 2; } // Anubis Cufflinks [MP reduction of 2 + 2%]
		if (CheckActorInventory(Player1TID,"BalrogRing") > 0) { Check1 += 5; Check2 -= 5; } // Balrog's Ring [MP reduction of 5 + 5%]

		FinalSpellCost -= Check1;
		FinalSpellCost = FinalSpellCost * Check2 / 100;

		// Ruinous Shield = +50% MP Consumption
		if (CheckActorInventory(Player1TID,"RuinousShield") > 0) FinalSpellCost = FinalSpellCost * 3 / 2;
		if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [FloraArmors]");
	}

	int MaxManaDouble = MaxMana * 2;
	if (SpellIsSelected == 53 && PowerupTimer[7] > 0)
	{
		if (FinalSpellCost > MaxManaDouble) FinalSpellCost = MaxManaDouble;
	}
	else
	{
		if (FinalSpellCost > MaxMana) FinalSpellCost = MaxMana;
	}
	if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [MaxManaClamp]");

	if (TrueGameSkill <= SKILL_VERY_EASY) SpellCostMinimum = OriginalSpellCost * 42 / 100;
	if (TrueGameSkill == SKILL_EASY) SpellCostMinimum =  OriginalSpellCost * 49 / 100;
	if (TrueGameSkill == SKILL_NORMAL) SpellCostMinimum =  OriginalSpellCost * 56 / 100;
	if (TrueGameSkill == SKILL_HARD) SpellCostMinimum =  OriginalSpellCost * 63 / 100;
	if (TrueGameSkill == SKILL_VERY_HARD) SpellCostMinimum = OriginalSpellCost * 70 / 100;
	if (TrueGameSkill == SKILL_EXTREME) SpellCostMinimum = OriginalSpellCost * 77 / 100;
	if (TrueGameSkill == SKILL_HORRIBLE) SpellCostMinimum = OriginalSpellCost * 84 / 100;
	if (TrueGameSkill == SKILL_IMPOSSIBLE) SpellCostMinimum = OriginalSpellCost * 91 / 100;
	if (TrueGameSkill >= SKILL_OMEGA) SpellCostMinimum = OriginalSpellCost * 98 / 100;

	// Clamp Minimum Mana Cost
	if (FinalSpellCost < SpellCostMinimum) FinalSpellCost = SpellCostMinimum;
	if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [CostMinimumClamp]");

	int SpellCostMinimum2 = 0;
	// Clearer of Mind Perk
	if (MiscellaneousVars[10055] > 0)
	{
		SpellCostMinimum2 = (MiscellaneousVars[10055] * 2) + (MiscellaneousVars[10056] * 5);
		if (MiscellaneousVars[10055] >= 2) SpellCostMinimum2 += 1;
		if (MiscellaneousVars[10055] >= 4) SpellCostMinimum2 += 2;
		if (MiscellaneousVars[10056] >= 2) SpellCostMinimum2 += 5;
		FinalSpellCost = FinalSpellCost * (100 - SpellCostMinimum2) / 100;
		if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [ClearMindChecks]");
	}
 
	// ((Sorcery + Therapeusis) / 2) = -0.1% Final Mana Cost [circumvents Clamp]
	StatManaCostReduction = 100 - ((MiscellaneousVars[34] + MiscellaneousVars[36]) / 20);
	if (StatManaCostReduction < 50) StatManaCostReduction = 50;
	FinalSpellCost = FinalSpellCost * StatManaCostReduction / 100;
	if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [SorcTherChecks]");

	// Checks Mana Cost Rate Menu Option [circumvents Clamp]
	FinalSpellCost = FinalSpellCost * GetCVar("dvds_manacostgmultiplier") / 100;
	if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [CostRateOption]");

	if (FinalSpellCost < 1) FinalSpellCost = 1;
	if (SpellIsSelected == 53 && PowerupTimer[7] > 0)
	{
		if (FinalSpellCost > MaxManaDouble) FinalSpellCost = MaxManaDouble;
	}
	else
	{
		if (FinalSpellCost > MaxMana) FinalSpellCost = MaxMana;
	}
	if (spellcostdebug == 1) PrintBold(s:"FinalSpellCost: ",d:FinalSpellCost,s:" [FinalClamps]");

	return FinalSpellCost;
}

