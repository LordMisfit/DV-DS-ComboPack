// Using a Diabloist's Hands' HP Gravity attack-projectile on an enemy
script "Hand-GravityAttack" (int type, int skillcheck)
{
	int CurrentHealth, GravityEffectMultiplier, GravityEffectFactor, PrinterOld, PrinterNew;

	PrinterOld = GetActorProperty(0,APROP_Health);
	if (skillcheck > 0)
	{
		if (TrueGameSkill <= SKILL_VERY_EASY) GravityEffectFactor = 875;  
		if (TrueGameSkill == SKILL_EASY) GravityEffectFactor = 750;  
		if (TrueGameSkill == SKILL_NORMAL) GravityEffectFactor = 500;  
		if (TrueGameSkill == SKILL_HARD) GravityEffectFactor = 250;  
		if (TrueGameSkill == SKILL_VERY_HARD) GravityEffectFactor = 125;  
		if (TrueGameSkill == SKILL_EXTREME) GravityEffectFactor = 100;  
		if (TrueGameSkill == SKILL_HORRIBLE) GravityEffectFactor = 75;  
		if (TrueGameSkill == SKILL_IMPOSSIBLE) GravityEffectFactor = 50;  
		if (TrueGameSkill >= SKILL_OMEGA) GravityEffectFactor = 20;
	}
	else GravityEffectFactor = 750;  

	GravityEffectMultiplier = 1000;
	GravityEffectMultiplier = GravityEffectMultiplier - GravityEffectFactor;

	CurrentHealth = GetActorProperty(0,APROP_Health);
	CurrentHealth = CurrentHealth * GravityEffectMultiplier / 1000;
	if (CurrentHealth < 1) CurrentHealth = 1;

	SetActorProperty(0,APROP_Health,CurrentHealth);
	PrinterNew = GetActorProperty(0,APROP_Health);
	// PrintBold(d:PrinterOld,s:"  ",d:PrinterNew);
	// SetActorState(0,"HPFlashGravity");
}

