			if (debuffsound == 1 && time > 0) MiscellaneousVars[25202]++;
			PrintBuffMessage(buffid);
		}
		if (type == 7) // Set timer [in tics] [do NOT ignore conditions like Buddha/God-Mode]
		{
			PowerupTimer[buffid] = time;
			if (debuffsound == 1 && time > 0) MiscellaneousVars[25202]++;
