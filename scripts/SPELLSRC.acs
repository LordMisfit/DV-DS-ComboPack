 }
 int buttons = GetPlayerInput(0, INPUT_BUTTONS);

 if ((PBA >= 0.0) && (PBA <= 64.0))
 {
  FWX = 1.0 - FixedMul(0.015625,PBA);
  FWY = 0.0 + FixedMul(0.015625,PBA);
