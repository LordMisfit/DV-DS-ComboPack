	int templevel;
	if (vimstone > 0)
	{
		templevel = MiscellaneousVars[181];
		if (vimstone == 1) 
		{
			templevel += level;
			if (templevel > 4) templevel = 4;
		}
		if (vimstone == 2) 
		{
			templevel += level;
			if (templevel > 7) templevel = 7;
		}
		level = templevel;
	}
	
	if (level < 0) level = 0;
	if (level > 7) level = 7;
	
				if (level == 1) FadeRange(254,104,253,0.33,254,104,253,0,1.25);
