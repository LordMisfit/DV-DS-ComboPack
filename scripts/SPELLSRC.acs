	{
		PlaySound(Player1TID,"tensionrevert",CHAN_BODY,1.0,0,ATTN_NORM);
	}
	
	if (level == 1) SpawnProjectile(0, "PlayerTension5BlastRadius", 0, 0, 0, 0, 2102);
	if (level == 2) SpawnProjectile(0, "PlayerTension20BlastRadius", 0, 0, 0, 0, 2102);
