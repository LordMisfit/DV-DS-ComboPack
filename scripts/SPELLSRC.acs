 	   if (ClassicDoomType == 4) { healpalused = 42; dmgpalused = 43; critdmgpalused = 44; } // Strife
	   if (ClassicDoomType == 5) { healpalused = 33; dmgpalused = 34; critdmgpalused = 35; } // Doom 64
	  }
      if (DmgIndDisplayType == 1) Thing_SetTranslation(NumberID, healpalused); // Healing [30/33]
      else // Damage
      {
       if (PlayerCrit > 0)
       {
        PlaySound(Player1TID,"player/criticalhit",CHAN_BODY,1.0,0,ATTN_NORM);
