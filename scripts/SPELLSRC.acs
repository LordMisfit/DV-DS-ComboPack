	if (type == 7) SpellCosts[value] |= perkvalue; // OR
	if (type == 8) SpellCosts[value] ^= perkvalue; // XOR
	if (type == 9) SpellCosts[value] <<= perkvalue; // Shift Left
	if (type == 10) SpellCosts[value] >>= perkvalue; // Shift Right
}



