			if (MiscellaneousVars[196] >= 60 && MiscellaneousVars[196] <= 74) flickeralpha = FixedMul(basealpha,0.5);
			if (MiscellaneousVars[196] >= 75 && MiscellaneousVars[196] <= 89) flickeralpha = FixedMul(basealpha,0.4375);
			if (MiscellaneousVars[196] >= 90 && MiscellaneousVars[196] <= 104) flickeralpha = FixedMul(basealpha,0.375);
