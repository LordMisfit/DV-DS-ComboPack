// Defines the Buildup to Sauravatar's appearance [DVI-DS]
script "DVI-MainEyeDeath2" (void)
{
	if (ModType != 0) // Deus Vult 1 or DVII
	{
		Delay(35);
		SetMusic("STAGE6D", 0); 
		AmbientSound("SauronAwakens",127);
		// 18.25 * 35 = 637
		Delay(637);
		ACS_NamedExecute("DVI-AwakenSauravatar",0,0,0,0);
		Delay(1);
	}
	else
	{
		Delay(21);
		Exit_Normal(0);
		Delay(1);
	}
}

