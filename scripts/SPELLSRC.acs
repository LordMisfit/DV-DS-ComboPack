					if (SpawnFixedX > -9217 && SpawnFixedX < 14337 && SpawnFixedY > -11777 && SpawnFixedY < 11777 && random(1,100) <= EventRand) { Thing_Damage(Player1TID,GameScriptEvents[1401],13); }
				}
