	if (debugevolvestealth == 1) PrintBold(s:"Evolve Chance [PeasentCheck]: ",d:SpawnChance,s:" - ",s:GetActorClass(0));
	
	if (debugevolvestealth == 1) PrintBold(s:"Evolve Chance [Final]: ",d:SpawnChance,s:" - ",s:GetActorClass(0));
