  delay(totaldelay); // 1
  If (GetActorProperty(0,APROP_Health) != PreviousHealth)
  {
     //if (CheckActorInventory(Player1TID,"DealCriticalHit") > 0 && IsPointerEqual(AAPTR_TARGET, AAPTR_PLAYER1) == TRUE && totaldelay == 1) PlayerCrit = 1; else PlayerCrit = 0;
