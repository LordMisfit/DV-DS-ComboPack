        HudMessage(s:"A";HUDMSG_ALPHA,C1+3302,cr_untranslated,1.0+I2,24.0,HUDMsgDelayFixed,msgalpha);
       }
     }
    }
   }

   G6 += 1; if (G6 > 11) G6 = 0; // Resets Palette Flasher
