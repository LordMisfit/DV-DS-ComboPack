	SpellIsSelected = checktype;
	
	if (SpellIsSelected == 53) // Divine Avatar Costs at most 90% of Mana Supply by Default
	{
		if (TrueGameSkill <= SKILL_VERY_EASY) DivineAvatarPercentCost = 80;
		if (TrueGameSkill == SKILL_EASY) DivineAvatarPercentCost = 85;
		if (TrueGameSkill == SKILL_NORMAL) DivineAvatarPercentCost = 90;
		if (TrueGameSkill == SKILL_HARD) DivineAvatarPercentCost = 92;
		if (TrueGameSkill == SKILL_VERY_HARD) DivineAvatarPercentCost = 95;
		if (TrueGameSkill == SKILL_EXTREME) DivineAvatarPercentCost = 97;
		if (TrueGameSkill == SKILL_HORRIBLE) DivineAvatarPercentCost = 98;
		if (TrueGameSkill == SKILL_IMPOSSIBLE) DivineAvatarPercentCost = 99;
		if (TrueGameSkill >= SKILL_OMEGA) DivineAvatarPercentCost = 100;
		
		if (PowerupTimer[7] > 0) DivineAvatarPercentCost *= 2;
		
		FinalSpellCost = MaxMana * DivineAvatarPercentCost / 100;
		OriginalSpellCost = MaxMana * DivineAvatarPercentCost / 100;
	}
