						SetFont("AIBFOCUS");
						HudMessage(s:"A";HUDMSG_ALPHA,6303,cr_untranslated,xpos,ypos,HUDMsgDelayFixed,msgalpha);
					}

					// Heat/MP Meter Border [6304]
					xpos = ScreenXOffset + 1289.0;
					ypos = ScreenYOffset + 1028.0; // 1060.0;
